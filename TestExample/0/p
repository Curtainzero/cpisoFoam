FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    "(input1|input2)"
    {
        type             zeroGradient;
    }

    output
    {
        type            fixedValue;
        value           $internalField;
    }

    test
    {
        type            zeroGradient;
    }

}

